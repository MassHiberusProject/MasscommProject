<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.masscomm.common.Cumpleanios" table="Cumpleanios" catalog="masscomm">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="nombre" type="string">
            <column name="nombre" length="20" not-null="false" unique="false" />
        </property>
        <property name="apellidos" type="string">
            <column name="apellidos" length="50" not-null="false" unique="false" />
        </property>
        <property name="empresa" type="string">
            <column name="empresa" length="50" not-null="false" unique="false" />
        </property>        
        <property name="fecha" type="date">
            <column name="fecha" not-null="false" unique="false" />
        </property>        
        
    </class>
</hibernate-mapping>