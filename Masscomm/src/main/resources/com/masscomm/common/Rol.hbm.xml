<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.masscomm.common.Rol" table="Rol" catalog="masscomm">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="rolname" type="string">
            <column name="rolname" length="30" not-null="true" unique="true" />
        </property>
        <set name="usuarios" table="UsuarioRol"
             inverse="true" lazy="true" fetch="select">
            <key column="rol_id" />
            <many-to-many column="usuario_id" class="com.masscomm.common.Usuario" />
        </set>
    </class>
</hibernate-mapping>